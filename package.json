{
	"name": "chatot",
	"version": "1.1.0",
	"license": "MIT",
	"author": "leiddx <leiddx@hotmail.com>",
	"description": "",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./bin/*": "./dist/src/bin/*",
		"./bundle/*": "./dist/src/bundle/*",
		"./lib/*": "./dist/src/lib/*",
		"./model/*": "./dist/src/model/*",
		"./router/*": "./dist/src/router/*",
		"./eslint.config.js": "./eslint.config.js"
	},
	"scripts": {
		"prepare": "npx husky",
		"install": "npm run build",
		"eslint": "npx tsc --noEmit && npx eslint **/*.ts --ignore-pattern dist",
		"test": "npx mocha test/**/*.ts",
		"release": "npx commit-and-tag-version --releaseCommitMessageFormat \"ðŸ³ chore(release): {{currentTag}}\"",
		"build": "npx tsc -p ./src",
		"build:cover": "nyc --reporter=html npm run test",
		"build:image": "docker build --progress=plain --no-cache -t %npm_package_name%:v%npm_package_version% .",
		"setup": "node --loader ts-node/esm ./bin/setup.js",
		"register": "node --loader ts-node/esm ./bin/register.js"
	},
	"homepage": "https://github.com/logedx/chatot#readme",
	"bugs": {
		"email": "leiddx@hotmail.com",
		"url": "https://github.com/logedx/chatot/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/logedx/chatot.git"
	},
	"directories": {
		"bin": "./bin",
		"doc": "./doc",
		"test": "./test"
	},
	"engines": {
		"node": ">= 22.0.0",
		"npm": ">= 10.0.0"
	},
	"dependencies": {
		"@inquirer/prompts": "^7.7.1",
		"@tensorflow/tfjs": "^4.22.0",
		"@types/ali-oss": "^6.16.11",
		"@types/compression": "^1.8.1",
		"@types/config": "^3.3.5",
		"@types/express": "^5.0.3",
		"@types/inquirer": "^9.0.8",
		"@types/mime-types": "^3.0.1",
		"@types/morgan": "^1.9.10",
		"@types/node": "^24.1.0",
		"@types/pinyin": "^2.10.2",
		"@types/xml2js": "^0.4.14",
		"ali-oss": "^6.23.0",
		"axios": "^1.11.0",
		"chalk": "^5.4.1",
		"compression": "^1.8.1",
		"config": "^4.1.0",
		"express": "^5.1.0",
		"inquirer": "^12.8.2",
		"mime-types": "^3.0.1",
		"moment": "^2.30.1",
		"mongoose": "^8.16.4",
		"morgan": "^1.10.1",
		"pinyin": "^4.0.0",
		"typescript": "^5.8.3",
		"xml2js": "^0.6.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.31.0",
		"@stylistic/eslint-plugin": "^5.2.2",
		"@types/chai": "^5.2.2",
		"@types/mocha": "^10.0.10",
		"@types/sinon": "^17.0.4",
		"chai": "^5.2.1",
		"commit-and-tag-version": "^12.5.1",
		"eslint": "^9.31.0",
		"husky": "^9.1.7",
		"mocha": "^11.7.1",
		"nyc": "^17.1.0",
		"ora": "^8.2.0",
		"sinon": "^21.0.0",
		"supertest": "^7.1.4",
		"ts-node": "^10.9.2",
		"typescript-eslint": "^8.38.0"
	},
	"mocha": {
		"ui": "bdd",
		"exit": true,
		"sort": true,
		"color": true,
		"asyncOnly": true,
		"ignore": "./.gitignore",
		"node-option": [
			"enable-source-maps",
			"loader=ts-node/esm"
		]
	},
	"commit-and-tag-version": {
		"parserOpts": {
			"headerPattern": "(\\w*)(?:\\(([\\w\\$\\.\\-\\* ]*)\\))?: (.*)$"
		}
	}
}
